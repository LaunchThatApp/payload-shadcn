{
  "name": "@launchthat.apps/payload-shadcn",
  "version": "0.1.9",
  "description": "Shadcn UI components for PayloadCMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    },
    "./components/ui/*": {
      "import": "./dist/components/ui/*.js",
      "require": "./dist/components/ui/*.js"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./rsc": {
      "import": "./dist/rsc/index.js",
      "require": "./dist/rsc/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaunchThatApp/payload-shadcn.git"
  },
  "bugs": {
    "url": "https://github.com/LaunchThatApp/payload-shadcn/issues"
  },
  "homepage": "https://github.com/LaunchThatApp/payload-shadcn#readme",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "pnpm clean && pnpm add-use-client && pnpm build:swc && pnpm build:types && pnpm copyfiles",
    "build:swc": "NODE_ENV=production swc ./src -d ./dist --config-file .swcrc --strip-leading-paths --copy-files",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "clean": "rimraf dist",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint src --ext .ts,.tsx",
    "add-use-client": "chmod +x ./add-use-client.sh && ./add-use-client.sh",
    "prepack": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "shadcn",
    "ui"
  ],
  "author": "Desmond Tatilian <dtatilian@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "embla-carousel-react": "8.0.0-rc22",
    "lucide-react": "^0.358.0",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.51.0",
    "react-resizable-panels": "^2.0.15",
    "recharts": "^2.12.3",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9"
  },
  "peerDependencies": {
    "@payloadcms/ui": "*",
    "next": "^14.0.0",
    "payload": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@payloadcms/next": "^3.0.0",
    "@payloadcms/ui": "^3.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.106",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "copyfiles": "^2.4.1",
    "next": "^14.0.4",
    "payload": "^2.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
